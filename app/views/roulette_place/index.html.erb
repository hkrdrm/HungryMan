<h1>Listing places</h1>

<table width="100%">
  <tr>
    <th>Name</th>
    <th>Category</th>
    <th>Address</th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.name %></td>
    <td><%= place.category %></td>
    <td><%= place.address %></td>
  </tr>
<% end %>
</table>
<br>
<%= link_to 'Spin', '/roulette_place/spin' %> || 
<%= link_to 'Add Category', '/roulette_place/AddCategory' %>

